.w100
  width 100%
.pr
  position relative
.pa
  position absolute
.pcy
  position absolute
  top:50%
  transform: translateY(-50%)
.pcx
  position absolute
  left:50%
  transform: translateX(-50%)
.pc
  position absolute
  top 50%
  left 50%
  transform translate(-50%, -50%)
.ta
  text-align: center
.va
  vertical-align: middle
.vt
  vertical-align: top
.no_data
  text-align: center
  margin-top 40px
.wrapper
  position relative
  min-width 100%
  min-height 100%
.eli
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;    
.nowrap
  white-space nowrap
.mg-auto
  margin: auto
.radius
  border-radius: 50%
.radius-16
  border-radius: 16px
.radius-8
  border-radius: 8px
.radius-4
  border-radius: 4px
for ft in 1..500
  .radius{ft}
    border-radius: ft * 1px
  .wid-per{ft}
    width: ft * 1%
  .hei-per{ft}
    height: ft * 1%
  .wid{ft}
    width: ft * 1px
  .hei{ft}
    height: ft * 1px
  .br{ft}
    border-radius: ft * 1px
  .z-ind{ft}
    z-index: ft
  .lh{ft}
    line-height: ft * 1px
  .border-white{ft}
    border: ft * 1px solid white
  .border-black{ft}
    border: ft * 1px solid black
for ft in 0..50   
  .pd{ft}-x
    padding-right 1px * ft
    padding-left 1px * ft
  .pd{ft}-y
    padding-top 1px * ft
    padding-bottom 1px * ft
  .pd{ft}
    padding: 1px * ft
  .pdt{ft}
    padding-top: 1px * ft
  .pdb{ft}
    padding-bottom: 1px * ft
  .pdr{ft}
    padding-right: 1px * ft
  .pdl{ft}
    padding-left: 1px * ft
  .mg{ft}-x
    margin-right 1px * ft
    margin-left 1px * ft
  .mg{ft}-y
    margin-top 1px * ft
    margin-bottom 1px * ft
  .mg{ft}
    margin: 1px * ft
  .mgt{ft}
    margin-top: 1px * ft
  .mgb{ft}
    margin-bottom: 1px * ft
  .mgr{ft}
    margin-right: 1px * ft
  .mgl{ft}
    margin-left: 1px * ft



