.btn {
  height: 40px;
  line-height: 40px;
  font-size: 14px;
  border-radius: 4px;
  text-align center;
  color: #fff;
  border: 0;
  background: linear-gradient(to bottom, #58C5EB 0%, #12ABDF 100%);
}
.cline1{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.container {
  position: relative;
  padding: 0.1px;
  font-size: 14px;
  width: 100%
  height: 100vh
  background: #F4F7FC
  overflow-y: auto
  overflow-x: hidden
  box-sizing border-box
  -webkit-overflow-scrolling: touch
}

.grey_bar {
  width: 100%;
  padding: 5px 0;
}
.grey_line {
  width: 100%;
  padding-bottom: 1px;
  background: #f4f4f4
}
.dashLine {
  border-bottom: 1px dashed #999999
}
.solidLine {
  border-bottom: 1px solid #dedede
}
input {
  background: rgba(0, 0, 0, 0)
}

button {
  padding: 0;
  border: none;
}

